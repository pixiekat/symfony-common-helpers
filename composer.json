{
    "name": "pixiekat/symfony-common-helpers",
    "description": "A collection of traits and other common files for Symfony applications.",
    "type": "symfony-bundle",
    "autoload": {
        "psr-4": {
            "Pixiekat\\SymfonyHelpers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pixiekat\\SymfonyHelpers\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Katherine Elizabeth",
            "email": "katie@pixiekitten.net"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "license": "proprietary",
    "require": {
        "php": "^8.2 || ^8.3 || ^8.4",
        "symfony/framework-bundle": "^6.4 || >=7.0",
        "symfonycasts/reset-password-bundle": "^1.22",
        "symfonycasts/verify-email-bundle": "^1.14"
    },
    "require-dev": {
        "kint-php/kint": "^6.0"
    }
}

